.switchtab{
	width:100vw;
	height:124rpx;
	background:rgba(255,255,255,1);
	box-shadow:0px 3px 3px 0px rgba(0, 0, 0, 0.09);
	padding: 12rpx 48rpx 10rpx 48rpx;
	justify-content: space-between;
}
.switchtab>view{
	text-align: center;
	width:104rpx;
	height:104rpx;
	background-repeat: no-repeat;
	background-size: 100% auto;
	background-position: 50% 50%;
}
.color-w{
	color: white!important;
}
.nav-txt{
	font-size:12px;
	font-family:PingFang;
	font-weight:400;
	color:rgba(81,81,81,1);
}

.flex-row {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-orient: horizontal;
    flex-direction: row;
}

.flex-col {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-orient: vertical;
    flex-direction: column;
}

.flex-grow-0 {
    min-width: 0;
    -webkit-box-flex: 0;
    -ms-flex-positive: 0;
    flex-grow: 0;
    -ms-flex-negative: 0;
    flex-shrink: 0;
   
}

.flex-grow-1 {
    min-width: 0;
    -webkit-box-flex: 1;
    -ms-flex-positive: 1;
    flex-grow: 1;
    -ms-flex-negative: 1;
    flex-shrink: 1;
}
.bg-img{
	background-repeat: no-repeat;
	background-size: 100%;
	background-position: 50% 50%;
}
.nav-icon{
	width: 104rpx;
	height: 104rpx;
}
.po-r{
	position: relative;
}
.nav-t{
	font-size: 24rpx;
	font-family: PingFang SC Regular;
	color: #565555;
	text-align: center;
	position: absolute;
	left: 0;
	right: 0;
	bottom: 0;
	margin: auto;
}