.pages{
  width: 100vw;
  height: 100vh;
  background-color: white;
  padding-top: 160rpx;
}

.ft18{
  opacity: 1;
  font-size: 36rpx;
  font-family: PingFang SC Medium;
  color: rgba(48,48,48,1);
  line-height: 56rpx;
}
.item-radio{
  display: block;
  width: 100%;
  height: 84rpx;
}
.item-list{
  padding-left: 23rpx;
  margin-top: 52rpx;
  border-bottom: 2rpx solid #f4f4f4;
  padding-bottom: 20rpx;
}
.detail{
  height: 34rpx;
  opacity: 1;
  font-size: 36rpx;
  font-family: PingFang SC Medium;
  color: rgba(48,48,48,1);
  line-height: 56rpx;
  margin-top: 46rpx;
}
.submit-btn{
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  margin: auto;
  height: 98rpx;
  opacity: 1;
  background: rgba(26,121,146,1);
  line-height: 98rpx;
  text-align: center;
  font-size: 36rpx;
  font-family: PingFang SC Regular;
  font-weight: bold;
  color: rgba(255,255,255,1);
}
textarea{
  width: 702rpx;
  height: 493rpx;
  background: rgba(255,255,255,1);
  display: block;
  margin:40rpx auto;
  font-size: 36rpx;
  font-family: PingFang SC Medium;
  color: #0f0f0f;
  padding: 46rpx 26rpx;
}
.placeholder-txt{
  opacity: 1;
  font-size: 30rpx;
  font-family: PingFang SC Medium;
  color: rgba(196,196,196,1);
}
input{
	width: 702rpx;
	height: 90rpx;
	display: block;
	margin: auto;
	background: rgba(255,255,255,1);
	font-size: 36rpx;
	font-family: PingFang SC Medium;
	padding: 0 26rpx;
}
.commit{
	opacity: 1;
	font-size: 24rpx;
	font-family: PingFang SC Medium;
	color: rgba(153,153,153,1);
	text-align: center;
	position: fixed;
	bottom: 150rpx;
	left: 0;
	right: 0;
	margin: auto;
}
.message{
	width: 100vw;
	height: 100vh;
	position: fixed;
	left: 0;
	right: 0;
	bottom: 0;
	top: 0;
	margin: auto;
	background: rgba(0,0,0,.6);
	z-index: 100;
}
.content{
	width: 620rpx;
	height: 400rpx;
	background: rgba(255,255,255,1);
	box-shadow: 0 0 4rpx #999;
	position: fixed;
	left: 0;
	right: 0;
	bottom: 0;
	top: 0;
	margin: auto;
	border-radius: 10rpx;
}
.content>view:nth-child(1){
	height: 38rpx;
	font-size: 42rpx;
	font-family: PingFang SC Medium;
	color: rgba(50,50,50,1);
	line-height: 38rpx;
	text-align: center;
}
.content>view:nth-child(2),.content>view:nth-child(3){
	font-size: 30rpx;
	font-family: PingFang SC Medium;
	color: rgba(48,48,48,1);
	text-align: center;
}
.line-h{
	height: 2rpx;
	background: #B0B0B0;
	margin-top:50rpx ;
}
.do{
	height: 108rpx;
	font-size: 36rpx;
	font-family: PingFang SC Medium;
	color: rgba(8,91,125,1);
	line-height: 108rpx;
	text-align: center;
}
.status-page{
	height: 72rpx;
	opacity: 1;
	font-size: 36rpx;
	font-family: PingFang SC Medium;
	color: rgba(48,48,48,1);
	position: fixed;
	left: 0;
	right: 0;
	bottom: 0;
	top: 0;
	margin: auto;
	text-align: center;
}
.warn{
	width: 68rpx;
	height: 68rpx;
	opacity: 1;
	margin:300rpx auto 0 auto;
	display: block;
}
.not-pass{
	font-size: 36rpx;
	font-family: PingFang SC Medium;
	color: rgba(48,48,48,1);
	text-align: center;
	margin-top: 30rpx;
}
.not-desc{
	opacity: 1;
	font-size: 30rpx;
	font-family: PingFang SC Medium;
	color: rgba(153,153,153,1);
	text-align: center;
	margin-top: 10rpx;
}
.resubmit{
	width: 340rpx;
	height: 88rpx;
	opacity: 1;
	font-size: 42rpx;
	font-family: PingFang SC Bold;
	color: rgba(255,255,255,1);
	margin: auto;
	background: #1A7992;
	text-align: center;
	line-height: 88rpx;
	border-radius: 40rpx;
	margin-top: 380rpx;
}
.check{
	width: 30rpx;
	height: 30rpx;
	display: inline-block;
	vertical-align: middle;
	margin-right: 6rpx;
}