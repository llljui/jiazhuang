.ft15{
	height: 30rpx;
	opacity: 1;
	font-size: 30rpx;
	font-family: PingFang SC Medium;
	color: #333333;
	line-height: 30rpx;
	margin-top: 20rpx;
}

.table{
	width: 720rpx;
	height: 348rpx;
	opacity: 1;
	margin-top: 16rpx;
	position: relative;
}
.table>image{
	position: absolute;
	width: 720rpx;
	height: 348rpx;
	left: 0;
	right: 0;
}
.table-end{
	width: 720rpx;
	height: 46rpx;
	opacity: 1;
	margin-top: 12rpx;
	position: relative;
}
.table-end>image{
	position: absolute;
	left: 0;
	top: 0;
	margin: auto;
	width: 720rpx;
	height: 46rpx;
}
.line-h{
	width: 1500rpx;
	height: 1px;
	opacity: 1;
	background: rgba(235,235,235,1);
	margin: 14rpx -200rpx;
}
.content{
	width: 100%;
	flex-wrap: wrap;
	justify-content: flex-start;
}
.block-num{
	width: 188rpx;
	height: 56rpx;
	opacity: 1;
	background-color:#E5E5E5;
	line-height: 56rpx;
	text-align: center;
	color: #666666;
	border-radius: 30rpx;
	font-size: 24rpx;
	font-family: PingFang SC Medium;
	color: rgba(102,102,102,1);
	margin: 16rpx 26rpx;
}
.book-value{
	width: 62rpx;
	height: 56rpx;
	background-color: #eee;
	display: inline-block;
	vertical-align: middle;
	font-size: 24rpx;
	font-family: PingFang SC Medium;
	color: rgba(48,48,48,1);
	text-align: center;
}
.ac{
	display: inline-block;
	vertical-align: middle;
	width: 60rpx;
	height: 56rpx;
	background-color: #1A7992;
	text-align: center;
	line-height: 50rpx;
	color: white;
}
.actions-in{
	margin-top: 48rpx;
}
.soon-book{
	width: 100vw;
	height: 98rpx;
	opacity: 1;
	background: rgba(26,121,146,1);
	line-height: 98rpx;
	text-align: center;
	color: white;
	position: fixed;
	bottom: 0;
	left: 0;
	right: 0;
	margin: auto;
	border-radius: 0;
}
.thead{
	height: 70rpx;
}
.thead>view{
	width: 120rpx;
	height: 70rpx;
	font-size: 24rpx;
	font-family: PingFang SC Regular;
	color: rgba(51,51,51,1);
	text-align: center;
}
.l-100{
	line-height: 70rpx;
}
.tbody{
	height: 48rpx;
	line-height: 48rpx;
}
.tbody>view{
	height: 38rpx;
	line-height: 38rpx;
	width: 120rpx;
	text-align: center;
	font-size: 24rpx;
	font-family: PingFang SC Medium;
	color: rgba(51,51,51,1);
}